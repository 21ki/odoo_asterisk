[PostgreSQL-odoo]
Description         = PostgreSQL connection to asterisk database
Driver              = PostgreSQL
Database            = {{ default .Env.DB_NAME "asterisk" }}
Servername          = {{ default .Env.DB_IP "db" }}
UserName            = {{ default .Env.DB_USER "asterisk" }}
Password            = {{ default .Env.DB_PASSWORD "asterisk" }}
Port                = {{ default .Env.DB_PORT "5432" }}
Protocol            = {{ default .Env.DB_VERSION "9.6" }}
ReadOnly            = No
RowVersioning       = No
ShowSystemTables    = No
ShowOidColumn       = No
FakeOidIndex        = No
ConnSettings        =
